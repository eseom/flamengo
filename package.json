{
  "name": "flamengo",
  "version": "0.0.5",
  "main": "index.js",
  "license": "MIT",
  "bin": {
    "flamengo": "contrib/flamengo"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "NODE_ENV=development nodemon --watch settings.js --watch src/server bin/server",
    "dev:packager": "NODE_ENV=development node bin/packager",
    "build": "NODE_ENV=production webpack --verbose --colors --display-error-details --config ./tools/webpack.production.js && babel src/server -d dist/server",
    "start": "NODE_ENV=production node bin/server",
    "test:node": "NODE_ENV=test mocha $(find src/server -name '*-test.js') --compilers js:./tools/babel-require",
    "hails": "hails"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eseom/flamengo"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "es2015": true
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-flow-strip-types"
    ]
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "bcrypt-nodejs": "^0.0.3",
    "catbox-redis": "^2.0.2",
    "cheerio": "^0.22.0",
    "hails": "^0.1.15",
    "hapi-nested-route": "^0.1.5",
    "highcharts": "^5.0.11",
    "inert": "^4.1.0",
    "joi": "^10.2.2",
    "lodash": "^4.17.4",
    "lodash.kebabcase": "^4.1.1",
    "mousetrap": "^1.6.1",
    "pretty-error": "^2.0.3",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.7.0",
    "sql-formatter": "^1.2.0",
    "sqlite3": "^3.1.8",
    "superagent": "^3.5.1",
    "vue": "^2.3.3",
    "vue-nes": "^0.1.1",
    "vue-router": "^2.5.3",
    "vue-server-renderer": "^2.3.3",
    "vue-spinner": "^1.0.2",
    "vue-superagent": "^1.2.0",
    "vue-template-compiler": "^2.3.3",
    "vue-toasted": "^1.0.27",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.24.1",
    "babel-loader": "^6.3.2",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.2",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "karma": "^1.7.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "sass-loader": "^6.0.5",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.2",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.1",
    "url-loader": "^0.5.8",
    "vue-loader": "^12.1.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.17.1"
  }
}
